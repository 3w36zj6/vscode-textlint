{
  "name": "vscode-textlint-server",
  "version": "0.6.4",
  "description": "Textlint Linter Server",
  "engines": {
    "node": "*"
  },
  "private": true,
  "main": "lib/server.js",
  "files": [
    "lib"
  ],
  "dependencies": {
    "glob": "^7.1.1",
    "vscode-languageserver": "^5.2.1"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/node": "^10.12.21",
    "rimraf": "^2.6.1",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1"
  },
  "scripts": {
    "prepublishOnly": "npm run clean && npm run compile",
    "clean": "rimraf lib",
    "compile": "tsc -p .",
    "watch": "tsc -watch -p ./",
    "lint": "tslint src/**.ts"
  }
}
